# Test Environment Configuration
# This file is used for E2E and integration tests

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_TIMEOUT=5000

# WebSocket Configuration - DISABLED for tests
VITE_WEBSOCKET_ENABLED=false
VITE_WEBSOCKET_URL=ws://localhost:8000/ws
VITE_WEBSOCKET_RECONNECT=false

# Feature Flags - All enabled for testing
VITE_FEATURE_AI_CHAT=true
VITE_FEATURE_ADVANCED_CHARTS=true
VITE_FEATURE_PAPER_TRADING=true
VITE_FEATURE_ADMIN_PANEL=true
VITE_FEATURE_BACKTESTING=true
VITE_FEATURE_PORTFOLIO_ANALYTICS=true
VITE_FEATURE_REAL_TIME_NOTIFICATIONS=true
VITE_FEATURE_MULTI_LANGUAGE=true

# Logging Configuration - Enhanced for debugging
VITE_LOG_LEVEL=debug
VITE_LOG_TO_FILE=true
VITE_LOG_BUFFER_SIZE=1000

# Development Tools
VITE_SHOW_DEV_TOOLS=true
VITE_MOCK_API=true

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_SLOW_RENDER_THRESHOLD=100 