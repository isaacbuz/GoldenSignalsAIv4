{
  "name": "goldensignalsai-frontend",
  "version": "3.0.0",
  "description": "Next-Generation Trading Dashboard for GoldenSignalsAI",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:force": "vite --force",
    "dev:clean": "rm -rf node_modules/.vite && vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:tdd": "vitest --watch --reporter=verbose",
    "test:graph": "vitest --run src/**/*.graph.test.ts",
    "test:integration": "vitest --run src/**/*.integration.test.ts",
    "test:unit": "vitest --run src/**/*.unit.test.ts",
    "test:quick": "vitest run --reporter=basic",
    "generate:component": "node scripts/generate-component.js",
    "dev:full": "npm run dev",
    "check:all": "npm run lint && npm run test:quick && npm run build",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "report": "./scripts/daily-report.sh",
    "context-check": "./check-context.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.14.1",
    "@lightningchart/lcjs": "^7.1.2",
    "@mui/icons-material": "^5.14.18",
    "@mui/lab": "^5.0.0-alpha.155",
    "@mui/material": "^5.14.18",
    "@mui/x-charts": "^6.18.1",
    "@mui/x-date-pickers": "^8.7.0",
    "@mui/x-date-pickers-pro": "^8.7.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.80.6",
    "@types/chart.js": "^2.9.41",
    "@types/d3": "^7.4.3",
    "@types/howler": "^2.2.12",
    "@types/react-redux": "^7.1.34",
    "@types/react-table": "^7.7.20",
    "@types/react-window": "^1.8.8",
    "@types/socket.io-client": "^1.4.36",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "d3": "^7.9.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^12.23.9",
    "highcharts": "^12.3.0",
    "highcharts-react-official": "^3.2.2",
    "howler": "^2.2.4",
    "lightweight-charts": "^4.2.3",
    "lucide-react": "^0.525.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^6.0.0",
    "react-grid-layout": "^1.5.2",
    "react-helmet-async": "^2.0.4",
    "react-hot-toast": "^2.5.2",
    "react-intersection-observer": "^9.16.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.20.1",
    "react-table": "^7.8.0",
    "react-virtualized": "^9.22.6",
    "react-window": "^1.8.11",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.11",
    "ws": "^8.18.3",
    "yahoo-finance2": "^2.13.3",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cypress/react": "^9.0.1",
    "@cypress/webpack-dev-server": "^4.1.0",
    "@faker-js/faker": "^9.8.0",
    "@heroicons/react": "^2.2.0",
    "@mswjs/data": "^0.16.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-virtualized": "^9.22.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/ui": "^1.6.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.4",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.2",
    "neo4j-driver": "^5.15.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-bundle-analyzer": "^1.0.0",
    "vite-plugin-pwa": "^0.17.4",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.6.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
