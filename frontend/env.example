# GoldenSignalsAI Frontend Environment Configuration
# Copy this file to .env.local for local development

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_TIMEOUT=30000

# WebSocket Configuration
VITE_WEBSOCKET_ENABLED=true
VITE_WEBSOCKET_URL=ws://localhost:8000/ws
VITE_WEBSOCKET_RECONNECT=true
VITE_WEBSOCKET_RECONNECT_INTERVAL=5000

# Feature Flags
VITE_FEATURE_AI_CHAT=true
VITE_FEATURE_ADVANCED_CHARTS=false
VITE_FEATURE_PAPER_TRADING=false
VITE_FEATURE_ADMIN_PANEL=false
VITE_FEATURE_BACKTESTING=true
VITE_FEATURE_PORTFOLIO_ANALYTICS=true
VITE_FEATURE_REAL_TIME_NOTIFICATIONS=true
VITE_FEATURE_MULTI_LANGUAGE=false

# Logging Configuration
VITE_LOG_LEVEL=info
VITE_LOG_TO_FILE=false
VITE_LOG_BUFFER_SIZE=100

# Development Tools
VITE_SHOW_DEV_TOOLS=false
VITE_MOCK_API=false

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_SLOW_RENDER_THRESHOLD=16 